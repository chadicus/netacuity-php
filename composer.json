{
    "name": "traderinteractive/netacuity",
    "description": "A client for NetAcuity, a geoip lookup service.",
    "keywords": ["geoip", "netacuity"],
    "authors": [
        {
            "name": "Spencer Rinehart",
            "email": "anubis@overthemonkey.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0||^8.0",
        "guzzlehttp/guzzle": "^6.3",
        "traderinteractive/util-arrays": "^3.1||^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": { "TraderInteractive\\NetAcuity\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "TraderInteractive\\NetAcuity\\Tests\\": "tests" }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs",
        "test": "vendor/bin/phpunit"
    }
}
